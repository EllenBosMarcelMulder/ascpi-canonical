{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "hex Crossed Link",
  "type": "object",
  "required": ["status", "activation"],
  "properties": {
    "status": {
      "type": "string",
      "enum": ["available", "inactive"]
    },
    "activation": {
      "type": "string",
      "const": "external_only"
    },
    "internal_trigger": {
      "type": "boolean",
      "const": false
    }
  },
  "additionalProperties": false
}